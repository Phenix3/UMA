{
    "type": "project",
    "name": "phenix3/uma",
    "description": "Site web de l'Universite de Maroua(UMA)",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "a2lix/translation-form-bundle": "^3.2",
        "api-platform/core": "^3.0",
        "apy/breadcrumbtrail-bundle": "^1.9",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "easycorp/easyadmin-bundle": "^4.7",
        "geertw/ip-anonymizer": "^1.1",
        "illuminate/collections": "^9.45",
        "intervention/image": "^2.7",
        "knplabs/doctrine-behaviors": "dev-master",
        "knplabs/knp-menu-bundle": "^3.2",
        "knplabs/knp-paginator-bundle": "^6.0",
        "league/commonmark": "^2.4",
        "league/glide": "^2.2",
        "lexik/jwt-authentication-bundle": "^2.16",
        "liip/imagine-bundle": "^2.9",
        "nelmio/cors-bundle": "^2.2",
        "nucleos/antispam-bundle": "^2.3",
        "pentatrion/vite-bundle": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.14",
        "prezent/grid-bundle": "^0.7.0",
        "sensio/framework-extra-bundle": "^6.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/messenger": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/rate-limiter": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/ux-live-component": "^2.6",
        "symfony/ux-react": "^2.6",
        "symfony/ux-translator": "^2.10",
        "symfony/ux-turbo": "^2.6",
        "symfony/ux-typed": "^2.6",
        "symfony/validator": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.2.*",
        "symfonycasts/reset-password-bundle": "^1.16",
        "symfonycasts/verify-email-bundle": "^1.12",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^3.4",
        "twig/html-extra": "^3.4",
        "twig/inky-extra": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/markdown-extra": "^3.4",
        "twig/string-extra": "^3.4",
        "vich/uploader-bundle": "^2.0",
        "webonyx/graphql-php": "^14.11"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "pestphp/pest-plugin": true,
            "endroid/installer": true,
            "phpstan/extension-installer": true,
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7",
        "dbrekelmans/bdi": "^1.0",
        "endroid/installer": "^1.4",
        "ergebnis/phpstan-rules": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.25",
        "hautelook/alice-bundle": "^2.11",
        "nunomaduro/phpinsights": "^2.8",
        "pestphp/pest": "^1.23",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.13",
        "phpro/grumphp": ">=v0.19",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^9.5",
        "sebastian/phpcpd": "^6.0",
        "seld/jsonlint": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.50",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.1.*",
        "theofidry/psysh-bundle": "^4.5",
        "zenstruck/foundry": "^1.35"
    }
}
